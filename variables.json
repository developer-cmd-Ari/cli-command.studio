{
    "TU_USUARIO": {
        "descripcion": "Usuario de GitHub o Docker Hub usado para login y tags.",
        "valores": [
            "tu_usuario_docker",
            "ariel",
            "juan"
        ],
        "default": "tu_usuario_docker"
    },
    "IMAGEN_REPO": {
        "descripcion": "Nombre del repositorio en el registry (ej: en GHCR).",
        "valores": [
            "mi-app",
            "mi-backend"
        ],
        "default": "mi-app"
    },
    "IMAGEN_LOCAL": {
        "descripcion": "Nombre de la imagen local con tag (ej: app:tag).",
        "valores": [
            "mi-app:latest",
            "backend:1.0"
        ],
        "default": "mi-app:latest"
    },
    "CONTENEDOR_ID": {
        "descripcion": "ID o Nombre de un contenedor de Docker.",
        "valores": [
            "contenedor_1",
            "db_postgres"
        ],
        "default": "contenedor_1"
    },
    "ID_O_TAG": {
        "descripcion": "ID o Tag de una imagen local de Docker.",
        "valores": [
            "mi-app:latest",
            "imagen_id"
        ],
        "default": "mi-app:latest"
    },
    "REPO_OWNER": {
        "descripcion": "Usuario o de la organización dueña del repositorio en GitHub.",
        "valores": [
            "tu_usuario_github",
            "mi-organizacion"
        ],
        "default": "tu_usuario_github"
    },
    "REPO_NAME": {
        "descripcion": "Nombre del repositorio a clonar o crear.",
        "valores": [
            "front-end-app",
            "docker-ci-cd"
        ],
        "default": "front-end-app"
    },
    "DIRECTORIO_LOCAL": {
        "descripcion": "Directorio local donde se clonará el repositorio (ej: '.')",
        "valores": [
            ".",
            "mi-proyecto-clonado"
        ],
        "default": "."
    },
    "PR_STATE": {
        "descripcion": "Estado de la Pull Request o Issue a listar.",
        "valores": [
            "open",
            "closed",
            "all"
        ],
        "default": "open"
    },
    "PR_TITLE": {
        "descripcion": "El título que llevará el nuevo Pull Request.",
        "valores": [
            "feat: Nueva funcionalidad X",
            "fix: Corrección de bug Y"
        ],
        "default": "feat: Nueva funcionalidad X"
    },
    "PR_BODY": {
        "descripcion": "La descripción (cuerpo) del Pull Request.",
        "valores": [
            "Implementa el ticket #123. Por favor, revisar.",
            "Requiere revisión de seguridad."
        ],
        "default": "Implementa el ticket #123. Por favor, revisar."
    },
    "PR_NUMBER": {
        "descripcion": "El número de Pull Request a ver o fusionar.",
        "valores": [
            "1",
            "2"
        ],
        "default": "1"
    },
    "MERGE_METHOD": {
        "descripcion": "El método para fusionar un Pull Request en GitHub.",
        "valores": [
            "merge",
            "squash",
            "rebase"
        ],
        "default": "merge"
    },
    "ISSUE_STATE": {
        "descripcion": "Estado de las Incidencias (Issues) a listar.",
        "valores": [
            "open",
            "closed",
            "all"
        ],
        "default": "open"
    },
    "ISSUE_TITLE": {
        "descripcion": "El título que llevará la nueva Incidencia.",
        "valores": [
            "Bug: Error 500 en producción",
            "Feature: Añadir modo oscuro"
        ],
        "default": "Bug: Error 500 en producción"
    },
    "ISSUE_BODY": {
        "descripcion": "La descripción de la Incidencia.",
        "valores": [
            "Pasos para reproducir:\n1. ...",
            "Requerimiento del cliente: ..."
        ],
        "default": "Pasos para reproducir:\n1. ..."
    },
    "ISSUE_NUMBER": {
        "descripcion": "El número de la Incidencia a cerrar o ver.",
        "valores": [
            "10",
            "11"
        ],
        "default": "10"
    },
    "COMMIT_MESSAGE": {
        "descripcion": "Mensaje descriptivo para el commit de Git.",
        "valores": [
            "docs: Actualizar README",
            "feat: Añadir característica X",
            "fix: Corregir error de login",
            "refactor: Limpieza de código"
        ],
        "default": "feat: Añadir característica X"
    },
    "BRANCH_NAME": {
        "descripcion": "Nombre de la rama Git a crear, cambiar, fusionar o eliminar.",
        "valores": [
            "develop",
            "feature/nueva-funcionalidad",
            "main",
            "rama_2"
        ],
        "default": "rama_2"
    },
    "REMOTE_NAME": {
        "descripcion": "Nombre del repositorio remoto (ej: 'origin').",
        "valores": [
            "https://github.com/developer-cmd-Ari/app-dev",
            "mi-servidor",
            "upstream"
        ],
        "default": "https://github.com/developer-cmd-Ari/app-dev"
    },
    "FILE_PATH": {
        "descripcion": "Ruta de un archivo o directorio. Usar '.' para todos los archivos.",
        "valores": [
            ".",
            "README.md",
            "src/app.py",
            "src/config.json"
        ],
        "default": "."
    },
    "REMOTE_URL": {
        "descripcion": "URL del repositorio remoto (HTTPS o SSH).",
        "valores": [
            "git@github.com:usuario/repo.git",
            "https://github.com/usuario/repo.git"
        ],
        "default": "git@github.com:usuario/repo.git"
    },
    "STASH_MESSAGE": {
        "descripcion": "Mensaje descriptivo para el stash de Git.",
        "valores": [
            "WIP: Cambios de feature X",
            "Limpieza de código",
            "Antes de pull"
        ],
        "default": "WIP: Cambios de feature X"
    },
    "NUMERO_COMMITS": {
        "descripcion": "Número de commits a deshacer (ej: 1 para el último).",
        "valores": [
            "1",
            "2"
        ],
        "default": "1"
    },
    "VARIABLE_CONFIG": {
        "descripcion": "Nombre de la variable de configuración global (ej: user.name, user.email).",
        "valores": [
            "user.name",
            "user.email",
            "core.editor"
        ],
        "default": "user.name"
    },
    "VALOR_CONFIG": {
        "descripcion": "Valor a establecer para la variable de configuración.",
        "valores": [
            "Tu Nombre",
            "tu.email@ejemplo.com",
            "nano"
        ],
        "default": "Tu Nombre"
    },
    "BRANCH_A": {
        "descripcion": "Nombre de la primera rama para comparar (ej: main).",
        "valores": [
            "main",
            "staging",
            "rama-antigua"
        ],
        "default": "main"
    },
    "BRANCH_B": {
        "descripcion": "Nombre de la segunda rama para comparar (ej: feature/nueva).",
        "valores": [
            "feature/nueva-funcionalidad",
            "hotfix/bug-critico"
        ],
        "default": "feature/nueva-funcionalidad"
    },
    "BRANCH_BASE": {
        "descripcion": "Rama base sobre la que se hará el rebase.",
        "valores": [
            "main",
            "develop"
        ],
        "default": "main"
    }
}